{
  "name": "mx-fe-bone-kit",
  "description": "the kit of mx-fe-bone",
  "author": "mx",
  "version": "0.8.1",
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sankuai.com/mx/mx-fe-bone-kit.git"
  },
  "license": "MIT",
  "dependencies": {
    "nokit": "0.20.0",
    "path-to-regexp": "1.2.1",
    "send": "0.13.0",
    "vane": "0.2.3"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "eslint": "1.7.3",
    "webpack": "1.12.2"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": 0,
      "no-debugger": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended"
  }
}
